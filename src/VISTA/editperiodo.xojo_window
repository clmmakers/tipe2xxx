		  Comboaula.Text=""
		  Combohorafin.Text=""
		  Combohorainicio.Text=""
		  Popupgrupomateria.ListIndex=-1
		  Comboperiodo.Text=""
		  Comboaula.DeleteAllRows
		  Combohorafin.DeleteAllRows
		  Combohorainicio.DeleteAllRows
		  Popupgrupomateria.DeleteAllRows
		  Comboperiodo.DeleteAllRows
		  me.Caption= "Nuevo"
		  cargacontroles
		End Sub
	#tag EndEvent
#tag EndEvents
